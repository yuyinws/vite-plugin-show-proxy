{
  "name": "vite-plugin-show-proxy",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@7.2.1",
  "description": "show vite proxy when running in dev mode",
  "author": "yuyinws",
  "license": "MIT",
  "homepage": "https://github.com/yuyinws/vite-plugin-show-proxy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyinws/vite-plugin-show-proxy.git"
  },
  "bugs": "https://github.com/yuyinws/vite-plugin-show-proxy/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "npm run build -- --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "kolorist": "^1.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  }
}
